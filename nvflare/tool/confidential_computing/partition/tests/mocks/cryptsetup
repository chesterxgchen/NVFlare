#!/bin/bash
# Mock cryptsetup
case "$1" in
    "benchmark")
        # Always succeed for supported ciphers
        case "$2" in
            "aes-xts-plain64") exit 0 ;;
            *) exit 1 ;;
        esac
        ;;
    "luksFormat")
        echo "Mock LUKS format" >&2
        exit 0
        ;;
    *) exit 0 ;;
esac 